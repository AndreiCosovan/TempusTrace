<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_tracking">

    <fragment
        android:id="@+id/navigation_tracking"
        android:name="com.example.tempustrace.ui.tracking.TrackingFragment"
        android:label="Tracking"
        tools:layout="@layout/fragment_tracking" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.tempustrace.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.tempustrace.ui.account.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />
